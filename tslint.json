{
	"extends": "tslint-xo",
	"rules": {
		"align": false,
		"typedef": false,
		"ban-types": false,
		"no-unsafe-any": false,
		"jsdoc-format": false,
		"variable-name": false,
		"completed-docs": [
			true,
			{
				"methods": {
					"privacies": [
						"public"
					]
				},
				"functions": {
					"visibilities": [
						"exported"
					]
				}
			}
		]
	}
}
