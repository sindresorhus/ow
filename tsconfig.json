{
	"extends": "@sindresorhus/tsconfig",
	"compilerOptions": {
		"outDir": "dist",
		"target": "ES2019",
		"module": "ESNext",
		"allowSyntheticDefaultImports": true,
		"lib": ["ES2020"]
	},
	"include": ["source"],
	"typedocOptions": {
		"out": "docs",
		"excludePrivate": true,
		"hideGenerator": true,
		"media": "media"
	}
}
