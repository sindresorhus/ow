{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"declaration": false,
		"removeComments": false,
		"pretty": true,
		"allowUnreachableCode": false,
		"allowUnusedLabels": false,
		"noImplicitAny": false,
		"noImplicitUseStrict": false,
		"noFallthroughCasesInSwitch": true,
		"allowSyntheticDefaultImports": true,
		"strictNullChecks": true,
		"outDir": "dist",
		"lib": [
			"es2015"
		]
	},
	"exclude": [
		"node_modules",
		"dist"
	]
}
